{
  "name": "vitest-browser-mode-stackblitz",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test:browser": "vitest --config=vitest.browser.config.mts",
    "test:browser:chrome": "cross-env BROWSER=chrome vitest --config=vitest.browser.config.mts",
    "test:browser:firefox": "cross-env BROWSER=firefox vitest --config=vitest.browser.config.mts",
    "test:browser:playwright": "cross-env BROWSER_PROVIDER=playwright vitest --config=vitest.browser.config.mts",
    "test:browser:playwright:chrome": "cross-env BROWSER_PROVIDER=playwright BROWSER=chrome vitest --config=vitest.browser.config.mts",
    "test:browser:playwright:firefox": "cross-env BROWSER_PROVIDER=playwright BROWSER=firefox vitest --config=vitest.browser.config.mts"
  },
  "dependencies": {
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "https://pkg.pr.new/@vitest/browser@8977",
    "@vitest/browser-playwright": "https://pkg.pr.new/@vitest/browser-playwright@8977",
    "@vitest/browser-preview": "https://pkg.pr.new/@vitest/browser-preview@8977",
    "@vitest/browser-webdriverio": "https://pkg.pr.new/@vitest/browser-webdriverio@8977",
    "@vue/tsconfig": "^0.8.1",
    "cross-env": "^10.1.0",
    "cross-spawn": "^7.0.6",
    "eslint": "^9.39.1",
    "open": "^10.2.0",
    "picocolors": "^1.1.1",
    "std-env": "^3.10.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "https://pkg.pr.new/vitest@8977",
    "vitest-browser-vue": "^2.0.1",
    "vue-tsc": "^3.1.0"
  },
  "stackblitz": {
    "startCommand": "npm install && npm run test:browser"
  }
}
